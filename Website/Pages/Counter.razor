@page "/counter"
@using BlazorAnimate


<h1>Counter</h1>



<Animate Animation="Animations.SlideUp" Delay="TimeSpan.FromSeconds(2)" Duration="TimeSpan.FromSeconds(0.5)" >
    <p>Current count: @currentCount</p>
</Animate>

<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
